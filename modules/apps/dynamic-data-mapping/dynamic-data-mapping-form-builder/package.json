{
	"dependencies": {
		"classnames": "^2.2.6",
		"clay-alert": "2.16.0",
		"clay-checkbox": "2.16.0",
		"clay-dropdown": "2.16.0",
		"clay-icon": "2.16.0",
		"clay-management-toolbar": "2.16.0",
		"clay-modal": "2.16.0",
		"clay-radio": "2.16.0",
		"metal-clipboard": "2.0.1",
		"metal-debounce": "^2.0.2",
		"metal-dom": "2.16.8",
		"metal-drag-drop": "3.3.1",
		"metal-events": "2.16.8",
		"metal-jsx": "2.16.8",
		"metal-position": "2.1.2",
		"metal-state": "2.16.8",
		"metal-useragent": "3.0.1",
		"object-hash": "^1.3.0"
	},
	"jest": {
		"moduleNameMapper": {
			"(.*)\\.soy$": "$1.soy.js",
			"fixtures/(.*)": "<rootDir>/test/metal/js/__fixtures__/$1.js",
			"mock/(.*)": "<rootDir>/test/metal/js/__mock__/$1.js",
			"source/(.*)": "<rootDir>/src/main/resources/META-INF/resources/js/$1.js"
		},
		"modulePathIgnorePatterns": [
			"/__fixtures__/",
			"/__mock__/",
			"/classes/"
		],
		"setupFiles": [
			"<rootDir>/jest-setup.config.js"
		],
		"setupFilesAfterEnv": [
			"<rootDir>jest-setup-after-env.config.js"
		],
		"snapshotSerializers": [
			"metal-jest-serializer"
		],
		"testPathIgnorePatterns": [
			"fieldEditedHandler",
			"fieldSetAddedHandler",
			"focusedField",
			"Calculator",
			"Evaluator",
			"FormRenderer",
			"LayoutProvider",
			"PageRenderer",
			"RuleBuilder",
			"RuleEditor",
			"RuleList",
			"SuccessPage"
		]
	},
	"name": "dynamic-data-mapping-form-builder",
	"scripts": {
		"build": "liferay-npm-scripts build",
		"checkFormat": "liferay-npm-scripts lint",
		"format": "liferay-npm-scripts format",
		"test": "npm run build-soy-tests && liferay-npm-scripts test",
		"build-soy": "metalsoy --soyDeps \"../../../node_modules/+(clay-*|frontend-js-web)/src/**/*.soy\"",
		"build-soy-tests": "npm run build-soy && metalsoy -s \"test/**/*.soy\" -d \"test\""
	},
	"version": "3.0.0"
}